  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Planeshifter/node-concept-net.git"
  },
  "keywords": [
    "ConceptNet",
    "semantic-web",
    "knowledge-base",
    "semantic-network",
    "RDF",
    "DBpedia"
  ],
